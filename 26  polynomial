#include <stdio.h>
#include <math.h>

void main()
{
    double a,b,c;
    double x1,x2;
 printf("put number in ax^2+bx+c (a,b,c) = ");
    scanf("%lf,%lf,%lf",&a,&b,&c);

    x1=((-b+(sqrt((b*b)-(4*a*c))))/(2*a));
    x2=((-b-(sqrt((b*b)-(4*a*c))))/(2*a));
    printf("The result of the polynomial %.0fx^2+%.0fx+%.0f is x1=%.2f and x2=%.2f",a,b,c,x1,x2);

    return 0;
}
